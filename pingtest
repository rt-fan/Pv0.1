import os

def pingtest(ip):
    response = os.system("ping -n 1 -w 20 " + ip)
    if response == 0:
        b = True
    else:
        b = False
    return b


f = open('ip.txt')
line = f.readline()
while line:
    if ping(line.strip()):
        print('ping ' + line.strip() + ' yes')
        # vendor(line.strip(), oid_test)
    else:
        print('ping ' + line.strip() + ' no')
    line = f.readline()
f.close()
